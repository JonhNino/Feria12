<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="feria">
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:xe"></property>
            <property name="javax.persistence.jdbc.user" value="feria_libro"></property>
            <property name="javax.persistence.jdbc.password" value="A"></property>
            <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"></property>
        </properties>

    </persistence-unit>
</persistence>
